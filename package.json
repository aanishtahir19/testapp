  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "dev": "parcel public/index.html -p 4000",
    "build": "rm -rf build && rm -rf dist &&  parcel build public/index.html --public-url ./ --dist-dir build"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@parcel/transformer-sass": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "parcel": "^2.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "postcss":{
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
